{
    "manifest_version": 3,
    "name": "Health Plus",
    "version": "0.1.0",
    "description": "Extensão para auxiliar o trabalho no setor de agendamentos do Posto de Saúde de Seberi. Desenvolvido por Uziel Ferreira do Prado.",
    "icons":
    {
        "256": "assets/Images/Icons/icon-256.png"
    },
    "permissions":
    [
        "storage"
    ],
    "action": {},
    "background":
    {
        "service_worker": "src/background/sw.js"
    },
    "content_scripts":
    [
        {
            "matches": ["https://rang.seberi.rs.gov.br/beneficioPaciente.xhtml"],
            "js": ["src/content_scripts/Rang/beneficioPaciente.xhtml.js"],
            "world": "MAIN"
        },
        {
            "matches": ["http://cialtouruguai.com.br/municipio/atendimentoEletronico.php*"],
            "js": ["src/content_scripts/Digifred/atendimentoEletronico.php.js"]
        },
        {
            "matches": ["http://cialtouruguai.com.br/publico/relAutorizacaoAtendimento.php*"],
            "js": ["src/content_scripts/Digifred/relAutorizacaoAtendimento.php.js"]
        }
    ]
}